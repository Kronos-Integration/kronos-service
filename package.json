{
  "name": "kronos-service",
  "version": "0.0.0-semantic-release",
  "description": "Base service implementation",
  "main": "dist/module.js",
  "module": "src/module.js",
  "scripts": {
    "cover": "nyc npm test",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "test": "mocha tests/*-test.js",
    "semantic-release": "semantic-release",
    "pretest": "rollup -c",
    "posttest": "markdown-doctest",
    "precover": "rollup -c",
    "prepare": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-service.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service#readme",
  "dependencies": {
    "fast-safe-stringify": "^1.2.1",
    "kronos-endpoint": "^2.14.11",
    "loglevel-mixin": "^2.0.1",
    "model-attributes": "^3.0.10",
    "registry-mixin": "^2.1.15",
    "statetransition-mixin": "^4.0.0"
  },
  "devDependencies": {
    "mocha": "^4.0.1",
    "chai": "^4.1.2",
    "jsdoc-to-markdown": "^3.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.51.0",
    "semantic-release": "^9.1.0",
    "nyc": "^11.3.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.9.0"
  },
  "keywords": [],
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
