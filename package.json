{
  "name": "@kronos-integration/service",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/module.mjs",
  "exports": {
    ".": "./src/module.mjs"
  },
  "description": "Base service implementation",
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/module.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/module.mjs"
  },
  "dependencies": {
    "@kronos-integration/endpoint": "^9.2.11",
    "@kronos-integration/interceptor": "^10.0.9",
    "loglevel-mixin": "^6.0.0",
    "model-attributes": "^4.1.7",
    "statetransition-mixin": "^6.0.10"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.4",
    "documentation": "^13.0.2",
    "semantic-release": "^17.2.1"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-github-action",
      "arlac77/template-kronos-component"
    ]
  }
}
