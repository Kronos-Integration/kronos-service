{
  "name": "kronos-service",
  "version": "0.0.0-semantic-release",
  "description": "Base service implementation",
  "main": "dist/module.js",
  "module": "src/module.js",
  "scripts": {
    "cover": "npm run build && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "build": "node_modules/rollup/bin/rollup --output=dist/module.js -c -- src/module.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md",
    "test": "npm run build && node_modules/.bin/mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service#readme",
  "dependencies": {
    "fast-safe-stringify": "^1.1.3",
    "kronos-endpoint": "^2.14.5",
    "loglevel-mixin": "^1.6.10",
    "model-attributes": "^3.0.1",
    "registry-mixin": "^2.1.8",
    "statetransition-mixin": "^3.1.11"
  },
  "devDependencies": {
    "cz-conventional-changelog": "1.2.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "chai": "3.5.0",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.39.1",
    "semantic-release": "^6.3.5"
  },
  "contributors": [{
    "name": "Torsten Link",
    "email": "torstenlink@gmx.de"
  }, {
    "name": "Markus Felten",
    "email": "markus.felten@gmx.de"
  }],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.1"
  },
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
